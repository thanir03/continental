{
  "expo": {
    "name": "continental-hotel",
    "slug": "continental-hotel",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "continental-app",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.continental.hotel",
      "config": {
        "googleMaps": {
            "apiKey": "AIzaSyCOHQ2j4CKfjOsuUCcb4UgAYOWIKwk3vSo"
        }
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "@react-native-google-signin/google-signin",
      ["expo-font", {
        "fonts": ["./assets/fonts/Montserrat.ttf"]
      }]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "7cb7bb8c-3939-47a9-82db-d55b078b97ae"
      }
    }
  }
}
